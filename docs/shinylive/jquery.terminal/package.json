    "cssnano": "^5.1.5",
    "emoji-datasource-twitter": "^14.0.0",
    "eslint": "^8.11.0",
    "gm": "^1.23.1",
    "jest": "^27.5.1",
    "jsonlint-mod": "^1.7.6",
    "postcss": "^8.4.12",
    "typescript": "^4.6.2",
    "uglify-js": "^3.15.3"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}
